{{#with features}}
<div><strong>Processor:</strong></div>
{{#with processor}}
<div>brand: <strong>{{brand}},</strong></div>
<div>model: <strong>{{model}},</strong></div>
<div>speed: <strong>{{speed}},</strong></div>
<div>cores: <strong>{{cores}},</strong></div>
<div>cache: <strong>{{cache}},</strong></div>
{{/with}}
<p><strong>Memory:</strong></p>
{{#with memory}}
<div>type: <strong>{{type}},</strong></div>
<div>size: <strong>{{size}},</strong></div>
