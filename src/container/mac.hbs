<div>size: <strong>{{size}},</strong></div>
{{/with}}
<p><strong>Display:</strong></p>
{{#with display}}
<div>size: <strong>{{size}},</strong></div>
<div>resolution: <strong>{{resolution}},</strong></div>
<div>technology: <strong>{{technoligy}},</strong></div>
<div>color space: <strong>{{color_space}},</strong></div>
<div>brightness: <strong>{{brightness}},</strong></div>
<div>refresh rate: <strong>{{refresh_rate}},</strong></div>
{{/with}}
<p><strong>Graphics</strong></p>
{{#with graphics}}
<div>brand: <strong>{{brand}},</strong></div>
<div>model: <strong>{{model}},</strong></div>
<div>memory: <strong>{{memory}},</strong></div>
{{/with}}
<p><strong>Ports:</strong></p>
{{#each ports}}
<div>type: <strong>{{type}},</strong></div>
<div>count: <strong>{{count}},</strong></div>
