<div>life: <strong>{{life}},</strong></div>
{{/with}}
<div>weight: <strong>{{weight}},</strong></div>
<p><strong>Dimensions:</strong></p>
{{#with dimensions}}
<div>height: <strong>{{height}},</strong></div>
<div>width: <strong>{{width}},</strong></div>
