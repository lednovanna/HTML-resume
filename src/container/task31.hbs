{{#each cards}}
<div class="row">
<div class="col-4">
<div class="card">
    <div class="card-header">{{name}}</div>
    {{#each items}}
    <ul class="list group">
            <li class="list-group-item">{{this}}</li>
        </ul>
    {{/each}}
 </div>   
</div>
</div>
{{/each}}
