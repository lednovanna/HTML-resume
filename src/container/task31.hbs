{{#with winner}}
<h2>{{title}}</h2>

{{#with table}}
<table class="table table-bordered">
    <thead>
        <tr>
            {{#each head}}
            <th>{{this}}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each user}}
        <tr>
            <th>{{place}}</th>
            <th>{{name}}</th>
            <th>{{surname}}</th>
            <th>{{nickname}}</th> 
        </tr>
        {{/each}}
    </tbody>
</table>
